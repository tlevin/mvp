{
  "name": "1P4Z",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "One Password For Zombies",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "1P4Z",
    "default_popup": "popup.html"
  }
}